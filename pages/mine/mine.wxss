/* pages/mine/mine.wxss */
page{
 /* background: rgb(243,243,243); */
}
.header{
  width: 100%;
  background: rgb(62, 160, 82);
  height:176rpx;
  display: flex;
  font-size: 32rpx;
}
image{
width: 100%;
height: 100%;
}
.icon{
  height: 96rpx;
  width: 96rpx;
  display: flex;
  flex-direction:column;
  padding-top: 50rpx;
  padding-left: 30rpx;
}
.name{
  display:flex;
  flex-direction:column;
  padding-top: 52rpx;
  padding-left: 30rpx;
  flex: 1;
}
.arrow{
  display: flex;
  padding-right: 40rpx;
  width: 32rpx;
  height: 32rpx;
  padding-top: 65rpx;
}
.blank{
  width: 100%;
  background: #E3E7E1;
}
.nav{
  width: 100%;
  /* height: 112rpx; */
  display: flex;
  padding-top: 14rpx;
  padding-bottom: 14rpx;
}
.nav_content text{
  display: flex;
  justify-content:center;
}
.nav_content{
  display: flex;
  flex-direction: column;
  flex: 1;
  height: 84rpx;
}
.nav_line{
  border-left: 1rpx solid #E8EBE6;
  border-right: 1rpx solid #E8EBE6;
}
.nav_bottom{
 font-size: 24rpx;
  color: #999999;
  font-family: PingFangSC-Regular; 
}
.nav_top{
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #333333;
}
.bar{
  margin-left: 30rpx;  
  height: 92rpx;
  width: 720rpx;
  display: flex;
  align-items: center; 
  font-family: PingFangSC-Regular;
 font-size: 16px;
 color: #333333;

}
.border{
  border-bottom: 1rpx solid #E3E7E1;
}
.content_icon{
  width: 48rpx;
  height: 48rpx;
  display: flex;
  padding-right: 28rpx;
  flex-direction: column;
}
.content_title{
  display: flex;
  flex: 1;
}
.content_arrow{
  display: flex;
  width: 32rpx;
  height: 32rpx;
  padding-right: 32rpx;
}
.tel{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333333;
  padding-top: 80rpx;
  width: 100%;
  text-align: center;
}
.green{
  color: green;
}