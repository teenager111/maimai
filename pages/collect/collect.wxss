/* pages/collect/collect.wxss */
/**index.wxss**/  
.swiper-tab{  
    width: 100%;  
    border-bottom: 2rpx solid #777777;  
    text-align: center;  
    line-height: 80rpx;}  
.swiper-tab-list{   
    display: inline-block;  
    width: 125rpx;  
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333333;  
}  
.on{ color: green;  
    border-bottom: 5rpx solid green;}  
  
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }  
/* .swiper-box view{  
    text-align: center;  
}   */
image{width: 100%;height: 100%;}
.lease{
  width: 100%;
  height: 208rpx;
  display: flex;
   align-items: center; 
}
.lease_img{
 width: 240rpx;
 height: 160rpx;
 display: flex;
 padding: 0 30rpx;
}
.lease_content{
  height: 160rpx;
  flex: 1;
  padding-right: 30rpx;
  position: relative;
}
.lease_title{
  display: block;
  font-size: 30rpx;
  color: #333333;
  width: 430rpx;
  /* justify-content:flex-start;  */
  /* white-space: pre-line; */
}
.lease_address{
  padding-top: 4rpx;
  font-size: 24rpx;
  color: #999999;
  display: block;
}
.lease_price{
  font-family: PingFang-SC-Regular;
  font-size: 36rpx;
  color: #55A32A;
  position: absolute;
  bottom: 0;
}
.lease_update{
  position: absolute;
  font-size: 24rpx;
  color: #999999;
  bottom: 0;
  right: 10%;
}
.seekrend{
  width: 100%;
  height:200rpx;
  font-size: 24rpx; 
  display: flex;
  align-items: center;
}
.seekrend_left{
  flex:1;
  padding-left: 30rpx;
  height: 160rpx;
}
.seekrend_color{
  color: #999999;
}
.seekrend_num{
background: #61BA31;
border-radius: 4rpx;
color: #FFFFFF;
width: 60rpx;
height: 32rpx;
line-height: 32rpx;
justify-content: center;
display:inline-flex;
}
.seekrend_title{
font-size: 30rpx;
padding-left: 12rpx;
color: #0D0D0D;
display: inline-flex;
}
.seekrend_place{
  padding-top: 16rpx;
}
.seekrend_startTime{
  padding-top: 4rpx;
}
.seekrend_timeLimit{
  padding-top: 4rpx;
}
.seekrend_right{
  width: 160rpx;
  height: 160rpx;
  padding-right: 30rpx;
  position: relative;
}
.seekrend_date{
position: absolute;
bottom: 0;
right:10%;
color: #999999;
}